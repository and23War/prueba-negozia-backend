    "reflect-metadata": "^0.1.10",
    "rotating-file-stream": "^1.4.1",
    "serverless-http": "^2.5.0",
    "ts-node": "8.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6",
    "typeorm": "0.2.18",
    "winston": "^3.2.1"
