{
  "name": "prueba-negozia-backend",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "dev": "set debug=* && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./index.ts",
    "start": "ts-node -r tsconfig-paths/register ./index.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/bluebird": "^3.5.27",
    "@types/compression": "1.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/lodash": "^4.14.123",
    "@types/morgan": "^1.7.35",
    "@types/validator": "^10.11.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1"
  },
  "dependencies": {
    "@types/node": "^12.7.0",
    "bcrypt-nodejs": "0.0.3",
